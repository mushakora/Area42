#back{
    width: 90px;
    height: 90px;
    position: fixed;
    right: 0;
    bottom: 0;
    opacity: 0.6;
}
#back a{
    position: relative;
    display: block;
    width: 90px;
    height: 90px;
    text-decoration: none;
}
#back a::before{
    font-family: 'Font Awesome 5 Free';
    font-weight: 900;
    content: '\f064';
    font-size: 25px;
    color: #3f98ef;
    position: absolute;
    width: 25px;
    height: 25px;
    top: -40px;
    bottom: 0;
    right: 0;
    left: 0;
    margin: auto;
    text-align: center;
}
#back a::after{
    content: '上へ戻る';
    font-size: 13px;
    color: #fff;
    position: absolute;
    top: 45px;
    bottom: 0;
    right: 0;
    left: 0;
    margin: auto;
    text-align: center;
    color: #3f98ef;
}

