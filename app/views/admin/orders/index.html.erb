<div class="container">
  <div class="row">
	<h3>発注一覧</h3>
	  <table class="table">
		<thead>
		  <tr>
			<th>発注日</th>
		  </tr>
		</thead>

		<tbody>
		  <% @orders.each do |order| %>
			<tr>
			  <td><%= link_to order.created_at.strftime('%Y/%m/%d'), order_path(order.id) %></td>
			</tr>
		  <% end %>
		  <%= paginate @orders, class: "pagenate" %>
		</tbody>
	  </table>
  </div>
</div>
